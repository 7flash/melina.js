{
  "name": "melina",
  "version": "2.1.0",
  "description": "A lightweight web framework for Bun with Next.js-style routing and zero-config builds.",
  "module": "./src/web.ts",
  "main": "./src/web.ts",
  "bin": {
    "melina": "./bin/melina"
  },
  "files": [
    "src",
    "bin",
    "docs",
    "README.md",
    "SKILL.md"
  ],
  "exports": {
    ".": "./src/web.ts",
    "./web": "./src/web.ts",
    "./client": "./src/client/index.ts",
    "./client/render": "./src/client/render.ts",
    "./client/ssr": "./src/client/ssr.ts",
    "./client/types": "./src/client/types.ts",
    "./client/jsx-runtime": "./src/client/jsx-runtime.ts",
    "./client/jsx-dev-runtime": "./src/client/jsx-dev-runtime.ts",
    "./jsx-dom": "./src/jsx-dom.ts",
    "./Link": "./src/Link.tsx",
    "./utils": "./src/utils.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nicholasgriffintn/melina.js"
  },
  "author": {
    "name": "Melina.js Contributors"
  },
  "bugs": "https://github.com/nicholasgriffintn/melina.js/issues",
  "homepage": "https://github.com/nicholasgriffintn/melina.js#readme",
  "license": "MIT",
  "keywords": [
    "bun",
    "server",
    "framework",
    "typescript",
    "web",
    "ssr",
    "jsx",
    "app-router",
    "view-transitions",
    "melina"
  ],
  "type": "module",
  "private": false,
  "sideEffects": false,
  "devDependencies": {
    "@types/bun": "latest",
    "jsdom": "^28.1.0"
  },
  "dependencies": {
    "@tailwindcss/postcss": "^4.1.10",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "ts-dedent": "^2.2.0"
  }
}
{
  "name": "melina",
  "version": "2.3.0",
  "description": "A lightweight web framework for Bun with Next.js-style routing and zero-config builds.",
  "module": "./src/web.ts",
  "main": "./src/web.ts",
  "bin": {
    "melina": "./bin/melina"
  },
  "files": [
    "src",
    "bin",
    "docs",
    "README.md",
    "SKILL.md"
  ],
  "exports": {
    ".": "./src/web.ts",
    "./web": "./src/web.ts",
    "./server": "./src/server/index.ts",
    "./server/build": "./src/server/build.ts",
    "./server/serve": "./src/server/serve.ts",
    "./server/imports": "./src/server/imports.ts",
    "./server/router": "./src/server/router.ts",
    "./server/ssr": "./src/server/ssr.ts",
    "./client": "./src/client/index.ts",
    "./client/render": "./src/client/render.ts",
    "./client/types": "./src/client/types.ts",
    "./client/reconcilers": "./src/client/reconcilers/index.ts",
    "./client/jsx-runtime": "./src/client/jsx-runtime.ts",
    "./client/jsx-dev-runtime": "./src/client/jsx-dev-runtime.ts",
    "./client/jsx-dom": "./src/client/jsx-dom.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/7flash/melina.js"
  },
  "author": {
    "name": "Melina.js Contributors"
  },
  "bugs": "https://github.com/7flash/melina.js/issues",
  "homepage": "https://github.com/7flash/melina.js#readme",
  "license": "MIT",
  "keywords": [
    "bun",
    "server",
    "framework",
    "typescript",
    "web",
    "ssr",
    "jsx",
    "app-router",
    "view-transitions",
    "melina"
  ],
  "type": "module",
  "private": false,
  "sideEffects": false,
  "devDependencies": {
    "@types/bun": "latest",
    "jsdom": "^28.1.0",
    "melina": "link:."
  },
  "dependencies": {
    "@tailwindcss/postcss": "^4.1.10",
    "autoprefixer": "^10.4.21",
    "measure-fn": "3.5.0",
    "postcss": "^8.5.6",
    "ts-dedent": "^2.2.0"
  }
}
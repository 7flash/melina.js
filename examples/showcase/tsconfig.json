{
    "compilerOptions": {
        "jsx": "react-jsx",
        "jsxImportSource": "../../src/client",
        "target": "ESNext",
        "module": "ESNext",
        "moduleResolution": "bundler",
        "allowImportingTsExtensions": true,
        "strict": true,
        "noEmit": true,
        "skipLibCheck": true
    },
    "include": [
        "app/**/*",
        "server.ts"
    ]
}